{
  "expo": {
    "name": "workify-job-portal-react-native",
    "slug": "workify-job-portal-react-native",
    "owner": "gon28032004",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "scheme": "workify",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.workifyjobportalreactnative",
      "associatedDomains": [
        "applinks:workify"
      ],
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "workify",
              "com.anonymous.workifyjobportalreactnative"
            ]
          }
        ]
      }
    },
    "android": {
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "workify"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "com.anonymous.workifyjobportalreactnative"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "softwareKeyboardLayoutMode": "resize",
      "package": "com.anonymous.workifyjobportalreactnative"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "gradlePluginVersion": "8.5.2",
            "kotlinVersion": "1.9.24",
            "compileSdkVersion": 34,
            "targetSdkVersion": 34
          }
        }
      ],
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "8645e237-846b-4178-966d-59db5607a502"
      }
    }
  }
}